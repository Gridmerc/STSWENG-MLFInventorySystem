/div>\r\n                        </div>\r\n\r\n                        <input type=\"hidden\" id=\"totalSeconds\" value=\"totalSeconds\" />\r\n\r\n                        <div id=\"userMenuIcon\">\r\n                            <img id=\"logoImageGrey\" src=\"photo/logo-grey.png\" />\r\n                        </div>\r\n\r\n                        <div className=\"UserMenu\">\r\n                            <div id=\"userMenu\" className=\"d-flex flex-col text-left\">\r\n                                <div id=\"userIcon\" className=\"col\">\r\n                                    <img id=\"userIconImage\" src=\"../photo/icon-user.png\" />\r\n                                </div>\r\n\r\n                                <NavDropdown title=\"User\" id=\"user-dropdown\">\r\n                                <NavDropdown.Item eventKey=\"1.1\" onClick={this.handleSubmit}>Logout</NavDropdown.Item>\r\n                                </NavDropdown>\r\n\r\n                            </div>\r\n                        </div>\r\n\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        );\r\n    }\r\n}\r\n\r\nexport default Banner;","D:\\Documents\\La Salle\\5th Year\\Term 1\\STSWENG\\GitRepo\\STSWENG-MLFInventorySystem\\frontend\\src\\Components\\Partials\\Counter.js",[],"D:\\Documents\\La Salle\\5th Year\\Term 1\\STSWENG\\GitRepo\\STSWENG-MLFInventorySystem\\frontend\\src\\Components\\Admin\\AdminMenu.js",["98","99","100","101"],"import React, { Component, useState } from 'react';\r\nimport { Modal, Form, Card, Button, Nav } from 'react-bootstrap';\r\nimport manageArtistsWindow from \"./Artists/manageArtistsWindow\";\r\nimport manageItemsWindow from \"./manageItemsWindow\";\r\nimport manageBundlesWindow from \"./manageBundlesWindow\";\r\nimport manageEventsWindow from \"./manageEventsWindow\";\r\nimport salesReportWindow from \"../Home/salesReportWindow\";\r\n\r\nconst AdminMenu = () => {\r\n    const [manageArtistsShow, setManageArtistsShow] = us